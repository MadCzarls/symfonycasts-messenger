{
    "name": "madczarls/symfonycasts-messenger",
    "type": "project",
    "description": "Sandbox for Symfony Messenger learning - PHP 8.2 and Symfony 7.0",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer/package-versions-deprecated": "1.11.99.1",
        "doctrine/annotations": "^1.14",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^2.17",
        "intervention/image": "^2.7",
        "league/flysystem-bundle": "^3.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/amqp-messenger": "7.0.*",
        "symfony/asset": "7.0.*",
        "symfony/cache": "7.0.*",
        "symfony/config": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/dependency-injection": "7.0.*",
        "symfony/doctrine-bridge": "7.0.*",
        "symfony/doctrine-messenger": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/error-handler": "7.0.*",
        "symfony/event-dispatcher": "7.0.*",
        "symfony/filesystem": "7.0.*",
        "symfony/flex": "^1.21",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/http-client": "7.0.*",
        "symfony/http-foundation": "7.0.*",
        "symfony/http-kernel": "7.0.*",
        "symfony/intl": "7.0.*",
        "symfony/mailer": "7.0.*",
        "symfony/messenger": "7.0.*",
        "symfony/mime": "7.0.*",
        "symfony/monolog-bundle": "3.10.*",
        "symfony/monolog-bridge": "7.0.*",
        "symfony/notifier": "7.0.*",
        "symfony/options-resolver": "7.0.*",
        "symfony/process": "7.0.*",
        "symfony/property-access": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/routing": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/security-core": "7.0.*",
        "symfony/security-csrf": "7.0.*",
        "symfony/security-http": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/string": "7.0.*",
        "symfony/translation": "7.0.*",
        "symfony/twig-bridge": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/validator": "7.0.*",
        "symfony/web-link": "7.0.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "7.0.*",
        "twig/extra-bundle": "^3.8",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "7.0.*",
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "^1.53",
        "symfony/phpunit-bridge": "7.0.*",
        "symfony/stopwatch": "7.0.*",
        "symfony/var-dumper": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.0.*"
        },
        "public-dir": "public_html"
    }
}
